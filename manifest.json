{
  "name": "Chrome To Drive",
  "version": "1",
  "manifest_version": 2,
  "description": "Send files from Chrome directly to your drive!",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus"
  ]
}
